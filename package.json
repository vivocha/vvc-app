{
  "name": "@vivocha/client-interaction",
  "version": "7.11.5",
  "private": true,
  "description": "Vivocha Interaction Application",
  "scripts": {
    "prepare": "npm run build:prod",
    "clean": "[ ! -d ./dist/assets ] || rm -rf dist/assets/* && [ ! -d ./dist/ie ] || rm -rf dist/ie/*",
    "clean:all": "npm run clean && rm -rf node_modules",
    "cpmain": "cp ./src/main.scss ./dist/main.scss && cp ./src/manifest.json ./dist/manifest.json && cp ./src/strings.json ./dist/strings.json",
    "dist": "[ -d ./dist ] || mkdir ./dist",
    "build:app": "ng build client-interaction --delete-output-path=false",
    "build:app:ie": "ng build client-interaction-ie --delete-output-path=false",
    "prebuild": "npm run clean",
    "build": "npm run build:app && npm run build:app:ie",
    "postbuild": "npm run dist && cp ./src/main.html ./dist/main.html && npm run cpmain && node bannerize",
    "build:app:prod": "ng build client-interaction --prod --output-hashing none",
    "build:app:ie:prod": "ng build client-interaction-ie --prod --output-hashing none",
    "prebuild:prod": "npm run prebuild",
    "build:prod": "npm run build:app:prod && npm run build:app:ie:prod",
    "postbuild:prod": "npm run dist && cp ./src/main.prod.html ./dist/main.html && npm run cpmain && node bannerize",
    "build:app:watch": "ng build client-interaction --delete-output-path=false --preserve-symlinks --watch",
    "build:app:ie:watch": "ng build client-interaction-ie --delete-output-path=false --preserve-symlinks --watch",
    "prebuild:watch": "npm run prebuild && npm run dist && cp ./src/main.watch.html ./dist/main.html && npm run cpmain && echo \"\\033[0;31mPLEASE NOTE THAT build:watch may not work with IE11\\033[0m\"",
    "build:watch": "npm run build:app:watch",
    "build:all": "npm run clean:all && npm i",
    "precommit": "npm run build",
    "__precommit": "npm run build && npm run cover && npm run check-coverage",
    "dev": "ng serve client-interaction",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:debug": "ng test -sm=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vivocha/vvc-app.git"
  },
  "author": "Vivocha Dev Team <dev@vivocha.com> (http://www.vivocha.com/)",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/vivocha/vvc-app.git#readme",
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.2.0",
    "@angular-devkit/build-angular": "~0.803.19",
    "@angular/cli": "~8.3.19",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@babel/core": "~7.6.4",
    "@babel/preset-env": "~7.6.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "mkdirp": "^0.5.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "dependencies": {
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@ngrx/store": "^8.5.1",
    "@ngrx/store-devtools": "^8.5.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@vivocha/client-interaction-core": "^7.10.5",
    "@vivocha/client-interaction-layout": "7.7.1",
    "@vivocha/public-entities": "^7.1.3",
    "autolinker": "^1.6.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "3.3.3",
    "element-closest-polyfill": "^1.0.2",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  }
}
